                                //$("#Item").append('<li><a href="' + domainurl +'/'+state.Text + '">' + state.Value + '</a></li>');
                                $("#Item").append('<li class= "item-row" ><div class="row"><div class="col-md-12"><ul class><li><span> <a href="' + domainurl + '/' + state.Text + '">' + state.Value + '</a></dd></li></ul> </div> </div > </li >');

                            });
                        },
                        error: function (request, status, error)
